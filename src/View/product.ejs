<%- include('./client/head.ejs') %>
    <link rel="stylesheet" href="/sass/product.css">
    <%- include('./client/body.ejs') %>
        <section class="st-client-content">
            <div>
                <div class="p-5 bg-dark About bg-overlay">
                    <div class="container text-center text-light">
                        <h3 class="title">Steak In!</h3>
                        <h2 class="title">Product</h2>
                        <ol class="breadcrumb m-auto mt-5" arial-label="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item"><a href="#">Shop</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Product</li>
                        </ol>
                    </div>
                </div>
            </div>
            <section class="main container" id="main">
                <div class="d-flex flex-wrap jusitfy-content-center">
                    <div class="main__content--left col col-12 col-lg-9">
                        <div class="toolbar">
                            <form
                                class="d-flex flex-wrap gap-3 gap-xs-0 border border-1 toolbar__form d-f container row">
                                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-4 col-xs-12 gap-xs-1 toolbar__form--card d-flex"
                                    id="filter">
                                    <label for="filter" class="col-xs-4 col-sm-3 col-label">Sort by:</label>
                                    <div class="col__input col-xs-8 col-sm-9">
                                        <input class="col__input--selector" list="item-datalog" name="" id="filter"
                                            placeholder="Product name">
                                        <datalist id="item-datalog">
                                            <option value="Newest item"></option>
                                            <option value="Oldest item"></option>
                                            <option value="Hot item"></option>
                                            <option value="Highest rating"></option>
                                            <option value="Lowest item"></option>
                                        </datalist>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-4 toolbar__form--card d-flex"
                                    id="page-show">
                                    <label for="pagination" class="col-xs-4 col-sm-3 col-label">Show:</label>
                                    <div class="col__input col-xs-8 col-sm-9">
                                        <input class="col__input--selector" list="item-datalog" name="" id="pagination"
                                            placeholder="10 items / page">
                                        <datalist id="item-datalog">
                                            <option value="12 items / page"></option>
                                            <option value="15 items / page"></option>
                                            <option value="20 items / page"></option>
                                            <option value="30 items / page"></option>
                                        </datalist>
                                    </div>
                                </div>
                                <div class="col-sm-auto ms-auto col-xs-12" id="category-view">
                                    <label for="" class="col-label">View as:</label>
                                    <a class="view-as active" id="view-as-1"><i class="fa fa-th-large"></i></a>
                                    <a class="view-as" id="view-as-2"><i class="fa fa-th-list"></i></a>
                                </div>
                            </form>
                        </div>
                        <div class="product__items row">
                            <div class="item-1 item col-12 col-md-6 col-xs-12 col-lg-4">
                                <div class="img-wrapper">
                                    <img src="https://live.staticflickr.com/65535/51263261499_75554695fe_w.jpg" alt="">
                                    <a href="#" class="btn btn-cart">Add to Cart</a>
                                    <div class="img-wrapper__gray gray"></div>
                                </div>
                                <div class="item__content">
                                    <h3 class="item__content--title">
                                        2021’s Best Cookbooks
                                    </h3>
                                    <p class="item__content--msg">
